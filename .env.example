APP_ENV=dev

# Proxy auth (admin key for creating keys, etc.)
ADMIN_KEY=dev_admin_key_change_me

# Database
DATABASE_URL=postgresql+psycopg://proxy:proxy@postgres:5432/proxy

# Redis
REDIS_URL=redis://redis:6379/0

# Provider keys (your own, for V0)
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=...

# Config file path
PROXY_CONFIG_PATH=/app/concord.config.yaml
